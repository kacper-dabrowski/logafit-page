{
  "name": "logafit-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./",
    "check-types": "tsc --noEmit",
    "lint:fix": "eslint ./ --fix",
    "style:lint": "stylelint **/*.scss",
    "style:lint:fix": "stylelint --fix **/*.scss",
    "format": "prettier . --check",
    "format:fix": "prettier . --write -l",
    "checks": "npm run format && npm run lint && npm run style:lint && npm run check-types && npm run test",
    "fix": "npm run format:fix && npm run lint:fix && npm run style:lint:fix",
    "commit": "cz",
    "test": "jest",
    "report": "github-actions-ctrf pull-request test-results/summary.json"
  },
  "dependencies": {
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.10",
    "clsx": "^2.1.0",
    "contentful": "11.3.3",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.70.0",
    "sharp": "^0.33.2",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "29.5.14",
    "@types/node": "^20",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/parser": "^6.20.0",
    "commitizen": "^4.3.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "github-actions-ctrf": "0.0.54",
    "jest": "29.7.0",
    "jest-ctrf-json-reporter": "^0.0.9",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "prettier": "3.2.4",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false
    }
  }
}
